                     <template slot=\"title\">{{ subItem.title }}</template>\n                                <el-menu-item v-for=\"(threeItem,i) in subItem.subs\"\n                                              :key=\"i\"\n                                              :index=\"threeItem.index\">\n                                    {{ threeItem.title }}\n                                </el-menu-item>\n                            </el-submenu>\n                            <el-menu-item v-else\n                                          :index=\"subItem.index\"\n                                          :key=\"subItem.index\">\n                                {{ subItem.title }}\n                            </el-menu-item>\n                        </template>\n                    </el-submenu>\n                </template>\n                <template v-else>\n                    <el-menu-item :index=\"item.index\"\n                                  :key=\"item.index\">\n                        <i :class=\"item.icon\"></i><span slot=\"title\">{{ item.title }}</span>\n                    </el-menu-item>\n                </template>\n            </template>\n        </el-menu>\n    </div>\n</template>\n\n<script>\n    import bus from '../common/bus';\n    export default {\n        data () {\n            return {\n                collapse: false,\n                items: []\n            }\n        },\n        computed: {\n            onRoutes () {\n                return this.$route.path.replace('/', '');\n            }\n        },\n        created () {\n            // 通过 Event Bus 进行组件间通信，来折叠侧边栏\n            bus.$on('collapse', msg => {\n                this.collapse = msg;\n            }),\n            this.initRoute();\n        },\n        methods: {\n            initRoute () {\n                this.items = [\n                    {\n                        icon: 'el-icon-s-home',\n                        index: 'dashboard',\n                        title: '系统首页'\n                    },\n                    {\n                        icon: 'el-icon-user-solid',\n                        index: '96',\n                        title: '学校信息',\n                        subs: [\n                            {\n                                index: 'device',\n                                title: '设备信息'\n                            },\n                            {\n                                index: 'parentsInfo',\n                                title: '家长信息'\n                            },\n                            {\n                                index: 'studentsInfo',\n                                title: '学生信息'\n                            },\n                            {\n                                index: 'teacherAccount',\n                                title: '老师信息'\n                            },\n                            {\n                                index: 'companyNoticeInfo',\n                                title: '公告信息'\n                            },\n                            {\n                                index: 'alarmInfo',\n                                title: '告警信息'\n                            }\n                        ]\n                    },\n                    {\n                        icon: 'el-icon-s-claim',\n                        index: '31',\n                        title: '考勤管理',\n                        subs: [\n                            {\n                                index: 'machine',\n                                title: '考勤设备'\n                            },\n                            {\n                                index: 'attendanceInfo',\n                                title: '考勤信息'\n                            },\n                            {\n                                index: 'notAttendances',\n                                title: '未考勤信息'\n                            }\n                        ]\n                    },\n                    {\n                        icon: 'el-icon-user',\n                        index: '6',\n                        title: '账号管理',\n                        subs: [\n                            {\n                                index: 'agentAccount',\n                                title: '代理商'\n                            },\n                            {\n                                index: 'regulatoryAccount',\n                                title: '监管单位'\n                            },\n                            {\n                                index: 'schoolAccount',\n                                title: '学校账号'\n                            }\n                        ]\n                    },\n                    {\n                        icon: 'el-icon-chat-dot-square',\n                        index: '36',\n                        title: '信息管理',\n                        subs: [\n                            {\n                                index: 'feedBack',\n                                title: '意见反馈'\n                            },\n                            {\n                                index: 'commonProblem',\n                                title: '常见问题'\n                            },\n                            {\n                                index: 'features',\n                                title: '功能介绍'\n                            }\n                        ]\n                    },\n                    {\n                        icon: 'el-icon-s-tools',\n                        index: '4',\n                        title: '配置管理',\n                        subs: [\n                            {\n                                index: 'propType',\n                                title: '属性类型'\n                            },\n                            {\n                                index: 'propList',\n                                title: '属性列表'\n                            },\n                            {\n                                index: 'electricFenceList',\n                                title: '监管单位围栏'\n                            },\n                            {\n                                index: 'schoolElectricFenceList',\n                                title: '学校围栏'\n                            }\n                            ,\n                            {\n                                index: 'roundElectricFenceList',\n                                title: '圆形围栏'\n                            }\n                        ]\n                    },\n                    // {\n                    //   icon: 'el-icon-rank',\n                    //   index: '102',\n                    //   title: '路由管理',\n                    //   subs: [\n                    //     {\n                    //       index: 'routers',\n                    //       title: '路由'\n                    //     },\n                    //     {\n                    //       index: 'childrenRouters',\n                    //       title: '子路由'\n                    //     }\n                    //   ]\n                    // },\n                    // 额外添加的内容\n                    {\n                        icon :'el-icon-s-home',\n                        index:'manage',\n                        title:'分级管理'\n                    },\n                    {\n                        icon :'el-icon-mobile',\n                        index:'warningkind',\n                        title:'预警类别'\n                    },\n                    {\n                        icon :'el-icon-s-data',\n                        index:'6',\n                        title:'学生信息基础',\n                        subs: [\n                            {\n                                index: 'info',\n                                title: '信息浏览'\n                            },\n                            {\n                                index: 'input',\n                                title: '人工录入和编辑'\n                            },\n                            {\n                                index: 'upload',\n                                title: '自动导入(批量)'\n                            }\n                        ]\n                    },\n                    //\n                    {\n                        icon: 'el-icon-cpu',\n                        index: '56',\n                        title: '车载设备',\n                        subs: [\n                            {\n                                index: 'carDevice',\n                                title: '车载设备信息'\n                            }\n                        ]\n                    },\n                    {\n                        icon: 'el-icon-postcard',\n                        index: '57',\n                        title: '处警管理',\n                        subs: [\n                            {\n                                index: 'policePosition',\n                                title: '处警点信息'\n                            },\n                            {\n                                index: 'policePerson',\n                                title: '处警人员信息'\n                            },\n                            {\n                                index: 'alertInfo',\n                                title: '警情信息'\n                            }\n                        ]\n                    },\n                    {\n                        icon: 'el-icon-s-custom',\n                        index: '55',\n                        title: '个人中心',\n                        subs: [\n                            {\n                                index: 'companyPersonal',\n                                title: '个人信息'\n                            },\n                            {\n                                index: 'changePassword',\n                                title: '修改密码'\n                            },\n                            {\n                                index: 'changeControlTitleInfo',\n             