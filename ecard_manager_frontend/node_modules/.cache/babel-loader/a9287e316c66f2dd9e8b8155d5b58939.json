i/api/v1/schoolInfo/student';\n\n/* 家长信息地址 */\nconst parentUrl = apiDomainName + '/pcsitesapi/api/v1/schoolInfo/parent';\n\n/* 设备信息地址 */\nconst deviceInfoUrl = apiDomainName + '/pcsitesapi/api/v1/schoolInfo/device';\nconst imeiInfoUrl =\n    apiDomainName + '/pcsitesapi/api/v1/schoolInfo/student/imei';\n\n/* 考试信息 */\nconst examInfoUrl = apiDomainName + '/class/api/v1/exam';\n\n/* 考试成绩信息 */\nconst scoreInfoUrl = apiDomainName + '/class/api/v1/score';\n\n/* 作业信息地址 */\nconst taskUrl = apiDomainName + '/class/api/v1/homework';\n\n/* 公告信息地址 */\nconst noticeUrl = apiDomainName + '/school/api/v1/notice';\n\n/* 点评信息地址 */\nconst reviewInfoUrl = apiDomainName + '/class/api/v1/comment';\n\n/* 课程表信息地址 */\nconst courseInfoUrl = apiDomainName + '/class/api/v1/course';\n\n/* 考勤机信息地址 */\nconst machineInfoUrl = apiDomainName + '/pcsitesapi/api/v1/configInfo/machine';\n/* 考勤信息地址 */\nconst attendancesUrl = apiDomainName + '/school/api/v1/schoolAttendance';\n/* 未考勤信息地址 */\nconst notAttendancesUrl = apiDomainName + '/school/api/v1/notSchoolAttendance';\n\n/* 用户信息地址 */\nconst personInfo = apiDomainName + '/user/api/v1/controllers/info';\n\n/* 校验手机号 */\nconst phoneInfoUrl = apiDomainName + '/pcsitesapi/api/v1/phone';\n\n/* 获取所有设备地址*/\nconst deviceLocationInfoUrl =\n    apiDomainName + '/position/api/v1/controllers/device/location/manage';\n/* 获取某些设备的具体信息地址*/\nconst deviceLocationDetailInfoUrl =\n    apiDomainName + '/position/api/v1/controllers/device/list';\n\n/* 获取转移设备地址*/\nconst deviceMoveInfoUrl =\n    apiDomainName + '/position/api/v1/controllers/device/move/school';\nconst deviceMoveOldInfoUrl =\n    apiDomainName + '/position/api/v1/controllers/device/move/moveAny';\n\n/* 登录地址 */\nconst loginUrl = apiDomainName + '/api/login';\n\n/* 修改控制台信息地址 */\nconst dataTitleInfo = apiDomainName + '/pcsitesapi/api/v1/configInfo/dataTitle';\n\n/* 指令地址 */\nconst instructionUrl =\n    apiDomainName + '/position/api/v1/controllers/device/instruction';\n\n/* 最新定位数据地址 */\nconst locationUrl =\n    apiDomainName + '/position/api/v1/controllers/device/location';\n/* 获取轨迹回放地址 */\nconst trackListUrl =\n    apiDomainName + '/position/api/v1/controllers/device/track/list';\n\n/* 告警地址 */\nconst alarmInfoUrl =\n    apiDomainName + '/position/api/v1/controllers/device/almpush';\n/* 告警已读地址 */\nconst alarmHaveReadUrl =\n    apiDomainName + '/position/api/v1/controllers/device/almpush/read';\n/* 告警处理地址 */\nconst alarmHandleUrl =\n    apiDomainName + '/position/api/v1/controllers/device/almpush/deal';\n\n/* 意见反馈地址 */\nconst feedbackUrl = apiDomainName + '/user/api/v1/controllers/feedback';\n/* 常见问题地址 */\nconst problemUrl = apiDomainName + '/user/api/v1/controllers/problem';\n/* 功能介绍地址 */\nconst introductionUrl = apiDomainName + '/user/api/v1/controllers/introduction';\n\n/* 电子围栏 */\nconst electricFenceUrl =\n    apiDomainName + '/pcsitesapi/api/v1/configInfo/polygonFence';\n/* 学校电子围栏 */\nconst schoolElectricFenceUrl =\n    apiDomainName + '/pcsitesapi/api/v1/configInfo/schoolFence';\n/* 圆形围栏 */\nconst roundElectricFenceUrl =\n    apiDomainName + '/pcsitesapi/api/v1/configInfo/circularFence';\n\n/* 图片上传地址 */\nconst uploadUrl = apiDomainName + '/class/api/v1/img';\n\n\n/* 统计数量：老师 */\nconst teachersCountUrl = apiDomainName + '/pcsitesapi/api/v1/schoolInfo/teacher/count';\n\n/* 统计数量：学生 */\nconst studentsCountUrl = apiDomainName + '/api/student/count';\n\n/* 统计数量：设备 */\nconst devicesCountUrl = apiDomainName + '/api/device/count';\n\n\n/* 处警点地址 */\nconst alarmPointUrl =\n    apiDomainName + '/pcsitesapi/api/v1/configInfo/alarmPoint';\n/* 处警人员地址 */\nconst policeOfficerUrl =\n    apiDomainName + '/pcsitesapi/api/v1/configInfo/policeOfficer';\n/* 警情信息 */\nconst alertInfoUrl = apiDomainName + '/pcsitesapi/api/v1/configInfo/alertInfo';\n/* 处警人员位置地址 */\nconst policeOfficerPosiUrl = apiDomainName + '/position/api/v1/cj/manage';\n\nclass api {\n  /* 获取属性类型地址 */\n  getPropTypeUrl() { return propTypeUrl; }\n\n  /* 方法二：以后用这种方法统一管理api 目前先不优化了\n      api不仅仅提供地址，而是要提供请求后的数据\n      我在common中写有例子可提供参考put,delete,post请求自己仿造这写\n  */\n  getPropTypeUrl1(params) { return requestAll.getAxios1(feedbackUrl, params) }\n\n  /* 获取属性列表信息地址 */\n  getPropListUrl() { return propListUrl; }\n\n  /* 获取学校信息地址 */\n  getSchoolInfoUrl() { return schoolInfoUrl; }\n\n  /* 获取代理商信息地址 */\n  getAgentInfoUrlUrl() { return agentInfoUrl; }\n  /* 获取监管单位信息地址 */\n  getSupervisionInfoUrl() { return supervisionInfoUrl; }\n\n  /* 获取学校年级信息地址 */\n  getGradeInfoUrl() { return gradeInfoUrl; }\n\n  /* 获取学校班级信息地址 */\n  getClassInfoUrl() { return classInfoUrl; }\n\n  /* 获取学校老师信息地址 */\n  getTeacherInfoUrl() { return teacherUrl; }\n\n  /* 获取学校学科信息地址 */\n  getSubjectInfoUrl() { return subjectInfoUrl; }\n\n  /* 获取班级分配信息地址 */\n  getClassAllocationUrl() { return classAllocationoUrl; }\n\n  /* 获取学生信息地址 */\n  getStudentUrl() { return studentUrl; }\n\n  /* 获取学生家长信息地址 */\n  getParentUrl() { return parentUrl; }\n\n  /* 获取imei信息地址 */\n  getDeviceUrl() { return deviceInfoUrl; }\n\n  /* 获取imei信息地址 后期想改的时候再改*/\n  getImeiInfoUrl() { return imeiInfoUrl; }\n\n  /* 获取考试信息地址 */\n  getExamInfoUrl() { return examInfoUrl; }\n  /* 获取考试成绩信息地址 */\n  getScoreInfoUrl() { return scoreInfoUrl; }\n\n  /* 获取作业信息地址 */\n  getTaskUrl() { return taskUrl; }\n\n  /* 获取公告信息地址 */\n  getNoticeUrl() { return noticeUrl; }\n\n  /* 获取点评信息地址 */\n  getReviewInfoUrl() { return reviewInfoUrl; }\n\n  /* 获取课程表信息地址 */\n  getCourseInfoUrl() { return courseInfoUrl; }\n\n  /* 获取考勤机信息地址 */\n  getMachineInfoUrl() { return machineInfoUrl; }\n\n  /* 获取考勤信息地址 */\n  getAttendancesUrl() { return attendancesUrl; }\n\n  /* 获取未考勤信息地址 */\n  getNotAttendancesUrl() { return notAttendancesUrl; }\n\n  /* 获取用户个人信息地址 */\n  getPersonInfoUrl() { return personInfo; }\n\n  /* 获取登录信息地址 */\n  getLoginUrl() { return loginUrl; }\n\n  /* 获取手机号地址 */\n  getPhoneInfoUrl() { return phoneInfoUrl; }\n\n  /* 获取所有设备地址 */\n  getDeviceLocationInfoUrl() { return deviceLocationInfoUrl; }\n  /* 获取所有设备地址 */\n  getDeviceLocationDetailInfoUrl() { return deviceLocationDetailInfoUrl; }\n\n  /* 获取新的转移设备地址 */\n  getDeviceMoveInfoUrlUrl() { return deviceMoveInfoUrl; }\n  /* 获取旧的转移设备地址 */\n  getDeviceMoveOldInfoUrlUrl() { return deviceMoveOldInfoUrl; }\n\n  /* 获取重置密码地址 */\n  getDataTitleInfoUrl() { return dataTitleInfo; }\n\n  /* 获取指令地址 */\n  getInstructionUrl() { return instructionUrl; }\n\n  /* 获取最新定位数据地址 */\n  getLocationUrl() { return locationUrl; }\n\n  /* 获取轨迹回放数据地址 */\n  getTrackListUrl() { return trackListUrl; }\n\n  /* 获取告警信息地址 */\n  getAlarmInfoUrl() { return alarmInfoUrl; }\n  /* 获取告警已读信息地址 */\n  getAlarmHaveReadUrl() { return alarmHaveReadUrl; }\n  /* 获取告警处理信息地址 */\n  getAlarmHandleUrl() { return alarmHandleUrl; }\n\n  /* 获取意见反馈地址 */\n  getFeedbackUrl() { return feedbackUrl; }\n\n  /* 获取常见问题地址 */\n  getProblemUrl() { return problemUrl; }\n\n  /* 获取功能介绍信息地址 */\n  getIntroductionUrl() { return introductionUrl; }\n\n  /* 获取电子围栏地址 */\n  getElectricFenceUrl() { return electricFenceUrl; }\n\n  /* 获取学校电子围栏地址 */\n  getSchoolElectricFenceUrl() { return schoolElectricFenceUrl; }\n\n  /* 获取圆形电子围栏地址 */\n  getRoundElectricFenceUrl() { return roundElectricFenceUrl; }\n\n  /* 获取图片地址 */\n  getImgUrl() { return imgurl; }\n\n  /* 获取图片上传地址 */\n  getUploadUrl() { return uploadUrl; }\n  /* 老师数量统计 */\n  getTeachersCountUrl() { return teachersCountUrl; }\n  /* 学生数量统计 */\n  getStudentsCountUrl() { return studentsCountUrl; }\n  /* 设备数量统计 */\n  getDevicesCountUrl() { return devicesCountUrl; }\n  /* 处警点地址 */\n  getAlarmPointUrl() { return alarmPointUrl; }\n  /* 处警人员地址 */\n  getPoliceOfficerUrl() { return policeOfficerUrl; }\n  /* 警情信息地址 */\n  getAlertInfoUrl() { return alertInfoUrl; }\n  /*处警人员位置地址 */\n  getPoliceOfficerPosiUrl() { return policeOfficerPosiUrl; }\n}\n\n// 暴露接口\nexport default {api: api};\n"]}]}