J,CAAc,QAAd,EAAwBlB,KAAxB;AACAR,GAAG,CAAC0B,SAAJ,CAAc,QAAd,EAAwBjB,KAAxB;AACAT,GAAG,CAAC0B,SAAJ,CAAc,OAAd,EAAuBhB,IAAvB;AACAV,GAAG,CAAC0B,SAAJ,CAAc,OAAd,EAAuBf,IAAvB;AACAX,GAAG,CAAC0B,SAAJ,CAAc,SAAd,EAAyBd,MAAzB;AACAZ,GAAG,CAAC0B,SAAJ,CAAc,SAAd,EAAyBb,MAAzB;AACAb,GAAG,CAAC0B,SAAJ,CAAc,QAAd,EAAwBZ,KAAxB;AACAd,GAAG,CAAC0B,SAAJ,CAAc,SAAd,EAAyBX,MAAzB;AACAf,GAAG,CAAC0B,SAAJ,CAAc,WAAd,EAA2BV,QAA3B;AACAhB,GAAG,CAAC0B,SAAJ,CAAc,UAAd,EAA0BT,OAA1B,E,CAEA;;AACAd,KAAK,CAACwB,YAAN,CAAmBC,OAAnB,CAA2BR,GAA3B,CAA+B,UAAUF,MAAV,EAAkB;AAC/C,MAAIW,SAAS,GAAGX,MAAM,CAACY,GAAP,CAAWC,SAAX,CAAqBb,MAAM,CAACY,GAAP,CAAWE,WAAX,CAAuB,GAAvB,IAA8B,CAAnD,EAAsDd,MAAM,CAACY,GAAP,CAAWG,MAAjE,CAAhB;;AACA,MAAI,YAAYJ,SAAhB,EAA2B;AACzB,QAAIK,KAAK,GAAGC,MAAM,CAACC,YAAP,CAAoBC,OAApB,CAA4B,OAA5B,CAAZ;;AACA,QAAIH,KAAJ,EAAW;AACThB,MAAAA,MAAM,CAACoB,OAAP,CAAeC,aAAf,GAA+BL,KAA/B,CADS,CAC6B;;AACtC,aAAOhB,MAAP;AACD;AACF;;AACD,SAAOA,MAAP;AACD,CAVD,EAUG,UAAUsB,KAAV,EAAiB;AAClB,SAAOC,OAAO,CAACC,MAAR,CAAeF,KAAf,CAAP;AACD,CAZD,E,CAcA;;AACAtC,MAAM,CAACyC,UAAP,CAAkB,UAACC,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AACpC,MAAI,CAACV,YAAY,CAACC,OAAb,CAAqB,OAArB,CAAD,IAAkCO,EAAE,CAACG,IAAH,KAAY,OAAlD,EAA2D;AACzDD,IAAAA,IAAI,CAAC;AACHC,MAAAA,IAAI,EAAE;AADH,KAAD,CAAJ;AAGA;AACD;;AACD,MAAIX,YAAY,CAACC,OAAb,CAAqB,OAArB,KAAiCO,EAAE,CAACG,IAAH,KAAY,OAAjD,EAA0D;AACxDD,IAAAA,IAAI,CAAC;AACHC,MAAAA,IAAI,EAAE;AADH,KAAD,CAAJ;AAGA;AACD;;AACDD,EAAAA,IAAI;AACL,CAdD;AAgBA,IAAI9C,GAAJ,CAAQ;AACNE,EAAAA,MAAM,EAANA,MADM;AAEN8C,EAAAA,MAAM,EAAE,gBAAAC,CAAC;AAAA,WAAIA,CAAC,CAAChD,GAAD,CAAL;AAAA;AAFH,CAAR,EAGGiD,MAHH,CAGU,MAHV","sourcesContent":["import Vue from 'vue'\nimport App from './App.vue'\nimport router from './router';\nimport axios from 'axios';\nimport api from './utils/api.js';\nimport common from './utils/common.js';\nimport ElementUI from 'element-ui';\nimport { VChart, VLine, VArea, VBar, VPie, VPoint, VScale, VAxis, VGuide, VTooltip, VLegend } from 'vux'\n\nimport 'element-ui/lib/theme-chalk/index.css'; // 默认主题\nimport './assets/css/icon.css';\nimport './assets/font/iconfont.css';\n\nVue.config.productionTip = false\nVue.use(ElementUI, {\n  size: 'small'\n});\n\n/* 挂载到原型属性上 */\nVue.prototype.$api = api;\nVue.prototype.$common = common;\nVue.prototype.$axios = axios;\n\nVue.component('v-chart', VChart)\nVue.component('v-line', VLine)\nVue.component('v-area', VArea)\nVue.component('v-bar', VBar)\nVue.component('v-pie', VPie)\nVue.component('v-point', VPoint)\nVue.component('v-scale', VScale)\nVue.component('v-axis', VAxis)\nVue.component('v-guide', VGuide)\nVue.component('v-tooltip', VTooltip)\nVue.component('v-legend', VLegend)\n\n// http request拦截器 添加一个请求拦截器login路由跳过\naxios.interceptors.request.use(function (config) {\n  let filterUrl = config.url.substring(config.url.lastIndexOf(\"/\") + 1, config.url.length);\n  if ('login' !== filterUrl) {\n    let token = window.localStorage.getItem(\"token\");\n    if (token) {\n      config.headers.Authorization = token; //将token放到请求头发送给服务器\n      return config;\n    }\n  }\n  return config;\n}, function (error) {\n  return Promise.reject(error);\n})\n\n//使用钩子函数对路由进行跳转权限判断\nrouter.beforeEach((to, from, next) => {\n  if (!localStorage.getItem('token') && to.name !== 'Login') {\n    next({\n      name: 'Login'\n    })\n    return\n  }\n  if (localStorage.getItem('token') && to.name === 'Login') {\n    next({\n      name: 'Dashboard'\n    })\n    return\n  }\n  next()\n})\n\nnew Vue({\n  router,\n  render: h => h(App),\n}).$mount('#app')\n"]}]}